                    </div>
                    
                </li>
                <hr class="hr_line_training">
              
               
            </ul>
            </div>

        </div>

        <div class="col-md-4">

            <p class="display-6 text-center pt-4 text-muted fs-3">Admission Program</p>

            <div class="admission">
                <p class="title-admission">
                    Recent Posts
                </p>
                <ul class="admission-body">
                    <% postData.forEach(function(admission){ %>
                    <li>
                        <p class="fw-bold fs-6"><%= admission.heading %></p>
                        <p class="text-muted admission-description"><%= admission.description %></p>
                        <hr class="hr_line">
                    </li>

                    <% }) %>
                    
                </ul>
            </div>
        </div>

    </div>

